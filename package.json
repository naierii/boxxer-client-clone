{
  "name": "boxxer-client",
  "version": "8.1.0",
  "license": "MIT",
  "scripts": {
    "prepare": "patch-package",
    "ng": "ng",
    "ng-high-memory": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng",
    "start": "node server",
    "build": "ng build",
    "build:test": "run-s build:client:test build:admin:test build:server:test webpack html-minifier",
    "build:test:client": "run-s build:client:test build:server:test webpack html-minifier",
    "build:server": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng run client:server:production",
    "build:server:test": "ng run client:server:testing",
    "build:sitemap": "node sitemap.js",
    "git:version": "node git-version.js",
    "webpack": "webpack",
    "serve:admin": "ng serve admin --port 4201",
    "serve:client": "ng serve client",
    "serve:edge": "ng serve client -c edge",
    "serve:ios": "ng serve client -c ios --live-reload false --disable-host-check true --host 0.0.0.0",
    "serve:geny": "ng serve client -c geny --disable-host-check true --host 0.0.0.0",
    "bundle-report": "npm run build:stats && npm run show-report",
    "show-report": "webpack-bundle-analyzer dist/client/stats-es2015.json",
    "sm-report": "npm run build:sm && source-map-explorer dist/client/main.bundle.js",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "extract": "ngx-translate-extract -i ./src/client ./src/client -o ./src/i18n/en.json --clean --sort --format namespaced-json",
    "html-minifier": "html-minifier --input-dir dist/client --output-dir dist/client --file-ext html  --remove-comments --collapse-whitespace --collapse-boolean-attributes --remove-attribute-quotes --minify-js",
    "upgrade-interactive": "npm-check --update"
  },
  "private": true,
