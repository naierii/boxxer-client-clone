{
  "extends": "./tsconfig.app.json",
  "compilerOptions": {
    "outDir": "../../out-tsc/app-server",
    "baseUrl": ".",
    "module": "commonjs",
    "types": ["node"],
    "paths": {
      "@app/*": ["./../client/src/app/*"],
      "@global/*": ["./../global/*"],
      "@env/*": ["./../environments/*"],
      "@angular/*": ["./../../node_modules/@angular/*"],
      "@font/*": ["./../fortawesome/*"]
    },
    "sourceMap": false
  },
  "angularCompilerOptions": {
    "entryModule": "src/app/app.server.module#AppServerModule"
  }
}
